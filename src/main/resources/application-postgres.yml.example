spring:
  datasource:
    url: jdbc:postgresql://asgard_postgres:5432/asgard_db
    username: postgres
    password: your_password
    driver-class-name: org.postgresql.Driver
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        format_sql: true
    database-platform: org.hibernate.dialect.PostgreSQLDialect

ollama:
  api:
    url: http://asgardOllama:11434
  model: tinyllama:latest